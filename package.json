{
  "name": "rove",
  "description": "Name-based routing library",
  "version": "0.0.1",
  "author": "Chris Andrejewski <christopher.andrejewski@gmail.com>",
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "babel-preset-power-assert"
    ],
    "plugins": [],
    "env": {
      "nyc": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "bugs": {
    "url": "https://github.com/andrejewski/rove/issues"
  },
  "dependencies": {
    "power-assert": "^1.4.2"
  },
  "devDependencies": {
    "ava": "^3.1.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-unassert": "^2.1.2",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-power-assert": "^2.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babel-register": "^6.11.6",
    "babelify": "^8.0.0",
    "browserify": "^16.0.0",
    "del": "^3.0.0",
    "fixpack": "^2.3.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-sourcemaps": "^2.5.1",
    "nyc": "^11.0.2",
    "pre-commit": "^1.1.3",
    "pre-push": "^0.1.1",
    "source-map-support": "^0.5.0",
    "standard": "^11.0.0",
    "uglifyify": "^4.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "homepage": "https://github.com/andrejewski/rove#readme",
  "keywords": [
    "node",
    "npm",
    "package",
    "starter"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "pre-commit": [
    "prepublish"
  ],
  "pre-push": [
    "prepublish"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrejewski/rove.git"
  },
  "scripts": {
    "build": "gulp",
    "coveralls": "npm run report && nyc report --reporter=text-lcov | coveralls",
    "lint": "standard --fix && fixpack",
    "prepublish": "npm run build && npm run test",
    "report": "NODE_ENV=nyc nyc npm test",
    "test": "npm run lint && ava",
    "test-ci": "npm run prepublish",
    "watch": "gulp watch"
  },
  "standard": {
    "ignore": [
      "/docs/dist/**"
    ]
  }
}
